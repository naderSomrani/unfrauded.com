    <script>
        function sendEmail() {
            console.log("test")
            const name = document.getElementById("RequestName").value
            const company = document.getElementById("RequestCompany").value
            const email = document.getElementById("RequestEmail").value
            const message = document.getElementById("RequestMessage").value
            const requestSubject = document.getElementById("requestSubject").value
            const body = "Name: " + name + '\r\n' + "Company: " + company + "\r\n" + "Email: " + email + "\r\n" + "message: " + message
            Email.send({
                Host: "smtp.gmail.com",
                Username : "unfrauded.contact@gmail.com",
                Password : "oduthqsvbpbntzeh",
                To : 'contact@unfrauded.com',
                From : 'unfrauded.contact@gmail.com',
                Subject : requestSubject,
                Body : body
            }).then(
                console.log(email),
                document.getElementById("succesMessage").style.display = "block",
                message => alert("Demo request sent")
            );
        }
    </script>
