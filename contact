    let map;

    function initMap() {
      const myLatLng = { lat: 36.8944236, lng: 10.1862634 };
      map = new google.maps.Map(document.getElementById("map"), {
        center: myLatLng,
        zoom: 16,
      });
      new google.maps.Marker({
        position: myLatLng,
        map,
        label: 'U',
        title: "Unfrauded Office",
      });
    }
